<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.managment">
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/managementlogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/managementlogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Managment">
        <activity
            android:name=".Activities.UserRequest"
            android:exported="false" />
        <activity
            android:name=".Activities.ReqDefaultList"
            android:exported="false" />
        <activity
            android:name=".Activities.UserList"
            android:exported="false" />
        <activity
            android:name=".Activities.Splash"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.MainActivity"
            android:exported="true">
        </activity>
    </application>

</manifest>